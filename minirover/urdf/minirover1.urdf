<?xml version='1.0'?>
<robot name="gopigo">

  <!-- Based on gopigo3.urdf -->
  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <box size="0.2 0.11 0.005"/>
      </geometry>
    </visual>
  </link>

  <link name="porch_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <box size="0.2 0.11 0.005"/>
      </geometry>
    </visual>
  </link>

  <joint name="porch_link" type="fixed">
    <parent link="base_link"/>
    <child link="porch_link"/>
    <origin xyz="0 0.0 0.05" rpy="0 0 0" /> 
  </joint>

</robot>
